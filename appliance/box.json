{
    "stub": true,
    "main": "bin/console",
    "output": "../output/space.phar",
    "chmod": "0555",
    "exclude-composer-files": true,
    "exclude-dev-files": true,
    "dump-autoload": true,
    "directories": [
        "bin",
        "config",
        "domain",
        "features",
        "infrastructures",
        "public",
        "src",
        "templates",
        "translations"
    ],
    "files": [
        ".env",
        ".env.build",
        "composer.json",
        "composer.lock"
    ],
    "intercept": true,
    "alias": "space.phar",
    "compression": "GZ"
}
