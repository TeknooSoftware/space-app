{# templates/bundles/TeknooSpace/views/form.html.twig #}

{% extends '@TeknooSpace/dashboard.layout.html.twig' %}

{% import '@TeknooSpace/Functions/url.html.twig' as functions %}

{% block containerFluidInner %}
    <div class="layout-specing">
        <div class="d-md-flex justify-content-between align-items-center mb-3">
            <h5 class="mb-0">{% block instanceMainTitle '' %}</h5>

            <nav aria-label="breadcrumb" class="d-inline-block mt-2 mt-sm-0">
                <ul class="breadcrumb bg-transparent rounded mb-0 p-0">
                    <li class="breadcrumb-item text-capitalize"><a href="{{ path('space_dashboard') }}">{{ 'teknoo.space.text.space'|trans }}</a></li>
                    <li class="breadcrumb-item text-capitalize"><a href="{% block breadcrumbListLink '#' %}">{% block breadcrumbListTitle '' %}</a></li>
                    {% block breadcrumbAddsItems '' %}
                    <li class="breadcrumb-item text-capitalize active" aria-current="page">{% block breadcrumInstanceTitle '' %}</li>
                </ul>
            </nav>
        </div>

        {% block mainContent '' %}
    </div>
{% endblock %}{# containerFluidInner #}
