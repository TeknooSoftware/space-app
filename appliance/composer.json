{
    "name": "teknoo/space-app",
    "type": "project",
    "description": "Space is a Platform as a Service application built on Teknoo East PaaS, Teknoo Kubernetes Client libraries\nand Symfony. The application is multi-account, multi-users and multi-projects, to build and deploy projects on dedicated \ncontainerized platforms on Kubernetes cluster.",
    "keywords": ["paas","image","containerd","oci","docker","kubernetes","platform","service","ci", "cd","continuous delivery","platform.sh","heroku"],
    "homepage": "http://teknoo.software/space",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "richard@teknoo.software",
            "role": "Software architect, Lead developer"
        }
    ],
    "support": {
        "issues": "https://github.com/TeknooSoftware/space-app/issues",
        "source": "https://github.com/TeknooSoftware/space-app"
    },
    "funding": [
        {
            "type": "patreon",
            "url": "https://www.patreon.com/teknoo_software"
        },
        {
            "type": "github",
            "url": "https://github.com/sponsors/TeknooSoftware"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/annotations": "^1.14.3",
        "doctrine/common": "^3.4.3",
        "doctrine/mongodb-odm": "^2.5.2",
        "doctrine/mongodb-odm-bundle": "^4.5.2",
        "doctrine/persistence": "^3.1.4",
        "endroid/qr-code": "^4.8.2",
        "guzzlehttp/guzzle": "^7.6",
        "laminas/laminas-diactoros": "^2.25.2",
        "league/flysystem": "^3.15.1",
        "monolog/monolog": "^3.3.1",
        "php-di/php-di": "^6.4",
        "php-http/guzzle7-adapter": "^1.0",
        "php-http/message-factory": "^1.1",
        "phpdocumentor/reflection-docblock": "^5.3",
        "psr/http-client": "^1.0.2",
        "scheb/2fa-backup-code": "^6.8",
        "scheb/2fa-bundle": "^6.8",
        "scheb/2fa-google-authenticator": "^6.8",
        "scheb/2fa-totp": "^6.8",
        "spomky-labs/otphp": "^11.2",
        "symfony/amqp-messenger": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^2.2.5",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/html-sanitizer": "*",
        "symfony/http-client": "*",
        "symfony/intl": "*",
        "symfony/mailer": "*",
        "symfony/mercure-bundle": "^0.3.6",
        "symfony/messenger": "*",
        "symfony/mime": "*",
        "symfony/monolog-bundle": "*",
        "symfony/notifier": "*",
        "symfony/polyfill-apcu": "^1.27",
        "symfony/process": "*",
        "symfony/property-access": "*",
        "symfony/property-info": "*",
        "symfony/proxy-manager-bridge": "*",
        "symfony/psr-http-message-bridge": "^2.2",
        "symfony/runtime": "*",
        "symfony/security-bundle": "*",
        "symfony/serializer": "*",
        "symfony/string": "*",
        "symfony/templating": "*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/yaml": "*",
        "teknoo/bridge-phpdi-symfony": "^5.2.5",
        "teknoo/east-common-symfony": "^1.9.4",
        "teknoo/east-foundation-symfony": "^6.2.10",
        "teknoo/east-paas": "^1.6.1",
        "teknoo/immutable": "^3.0.16",
        "teknoo/kubernetes-client": "^1.3.4",
        "teknoo/recipe": "^4.2.7",
        "teknoo/states": "^6.0.19",
        "twig/extra-bundle": "^3.6",
        "twig/twig": "^3.6"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "behat/behat": "^3.13",
        "friends-of-behat/symfony-extension": "^2.4",
        "icanhazstring/composer-unused": "^0.8.8",
        "masterminds/html5": "^2.8",
        "nikic/php-parser": "^v4.15.4",
        "phpstan/phpstan": "^1.10.15",
        "phpstan/phpstan-symfony": "^1.3.1",
        "phpunit/phpunit": "^10.1.3",
        "phpunitgen/console": "~5.0",
        "squizlabs/php_codesniffer": "^3.7.2",
        "symfony/css-selector": "*",
        "symfony/debug-bundle": "*",
        "symfony/dom-crawler": "*",
        "symfony/error-handler": "*",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*",
        "zenstruck/messenger-test": "^1.7.2",
        "webmozart/assert": "^1.11"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Teknoo\\Space\\": "domain/",
            "Teknoo\\Space\\Infrastructures\\": "infrastructures/"
        },
        "classmap": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Teknoo\\Space\\Tests\\Behat\\": "tests/Behat/",
            "Teknoo\\Space\\Tests\\Unit\\App\\": "tests/src/",
            "Teknoo\\Space\\Tests\\Unit\\Infrastructures\\": "tests/infrastructures/",
            "Teknoo\\Space\\Tests\\Unit\\": "tests/domain/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "endpoint" : [
                "https://api.github.com/repos/TeknooSoftware/symfony-recipes/contents/index.json",
                "flex://defaults"
            ],
            "require": "^6.2"
        }
    }
}
